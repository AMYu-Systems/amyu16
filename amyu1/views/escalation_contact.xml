<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="escalation_contact_list_view" model="ir.ui.view">
            <field name="name">escalation.contact.list.view</field>
            <field name="model">escalation.contact</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="number_email"/>
                </tree>
            </field>
        </record>

        <record id="escalation_contact_form_view" model="ir.ui.view">
            <field name="name">escalation.contact.form.view</field>
            <field name="model">escalation.contact</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="number_email"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>

