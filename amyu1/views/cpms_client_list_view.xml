<odoo>
    <data>
        <record id="action_client_profile_card" model="ir.actions.report">
            <field name="name">Preview</field>
            <field name="model">client.profile</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">amyu1.report_client_profile_card</field>
            <field name="report_file">amyu1.report_client_profile_card</field>
            <field name="print_report_name">'Client Profile - %s' % (object.name)</field>
            <field name="binding_model_id" ref="model_client_profile"/>
        </record>

        <record id="client_tree_view" model="ir.ui.view">
            <field name="name">client.tree.view</field>
            <field name="model">client.profile</field>
            <field name="arch" type="xml">
                <tree create="False">
                    <field name="name"/>
                    <field name="associate_id"/>
                    <field name="supervisor_id" groups="amyu1.manager_group"/>
                    <field name="supervisor_id" groups="amyu1.cluster_group"/>
                </tree>
            </field>
        </record>

        <record id="client_kanban_view" model="ir.ui.view">
            <field name="name">client.kanban.view</field>
            <field name="model">client.profile</field>
            <field name="arch" type="xml">
                <kanban create="false">
                    <field name="name"/>
                    <field name="id"/>
                    <field name="image_101"/>
                    <kanban>
                        <field name="state_sequence" invisible="1"/>
                    </kanban>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_global_click">
                                <div class="oe_kanban_content">
                                    <div class="o_kanban_image">
                                        <field name="image_101" widget="image" options="{'zoom': True}"/>
                                    </div>
                                    <div class="oe_kanban_details">
                                        <div class="oe_kanban_title">
                                            <h6>
                                                <field name="name"/>
                                            </h6>
                                        </div>
                                        <div class="oe_kanban_description">
                                            <field name="client_system_generated"/>
                                        </div>
                                        <span style="font-weight: bold; color: blue;">
                                            <field name="state"/>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

