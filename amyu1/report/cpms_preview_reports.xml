<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_clientprofile_document">
        <t t-set="company_logo" t-value="logo"/>
        <t t-call="web.external_layout">
            <h1>
                I. General Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>Registered Name:</td>
                    <td>
                        <span t-field="doc.name"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Client ID:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.client_system_generated"/>
                    </td>
                </tr>
                <tr>
                    <td>Organizational Type:</td>
                    <td>
                        <span t-field="doc.organization_type"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Industry Class:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.industry_class"/>
                    </td>
                </tr>
                <tr>
                    <td>Date of Engagement:</td>
                    <td>
                        <span t-field="doc.date_of_engagement"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Nature of Activities, Brands, Product &amp; Services:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.nature_of_business"/>
                    </td>
                </tr>
            </table>
            <br/>
            <br/>
            <h1>
                II. Contact Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>Registered Address:</td>
                    <td>
                        <span t-field="doc.unit_no"/>
                        <span t-field="doc.building_name"/>
                        <span t-field="doc.street"/>
                        <span t-field="doc.district"/>
                        <span t-field="doc.city"/>
                        <span t-field="doc.zip"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Telephone:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.landline"/>
                    </td>
                </tr>
                <tr>
                    <td>Facsimile:</td>
                    <td>
                        <span t-field="doc.facsimile_no"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Website:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.website"/>
                    </td>
                </tr>
                <tr>
                    <td>Accounting or Office Admin Address:</td>
                    <td>
                        <span t-field="doc.unit_no2"/>
                        <span t-field="doc.building_name2"/>
                        <span t-field="doc.street2"/>
                        <span t-field="doc.district2"/>
                        <span t-field="doc.city2"/>
                        <span t-field="doc.zip2"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Primary Contact Person:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.primary_contact_person"/>
                    </td>
                </tr>
                <tr>
                    <td>Telephone:</td>
                    <td>
                        <span t-field="doc.landline2"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Mobile No:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.mobile_number"/>
                    </td>
                </tr>
                <tr>
                    <td>Email Address:</td>
                    <td>
                        <span t-field="doc.email_address"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Principal Accounting Officer:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.principal_accounting_officer"/>
                    </td>
                </tr>
                <tr>
                    <td>Telephone:</td>
                    <td>
                        <span t-field="doc.landline3"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Mobile No:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.mobile_number2"/>
                    </td>
                </tr>
                <tr>
                    <td>Email Address:</td>
                    <td>
                        <span t-field="doc.email_address2"/>
                    </td>
                </tr>
            </table>
            <br/>
            <br/>
            <h1>
                III. B.I.R Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>TAX ID No:</td>
                    <td>
                        <span t-field="doc.vat"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">RDO Code:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.rdo_code"/>
                    </td>
                </tr>
                <tr>
                    <td>Registration Date:</td>
                    <td>
                        <span t-field="doc.registration_date"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Taxpayer Type:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.taxpayer_type"/>
                    </td>
                </tr>
                <tr>
                    <td>Invoice Type:</td>
                    <td>
                        <span t-field="doc.invoice_tax"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;"><![CDATA[Filling & Payment:]]></td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.filing_payment"/>
                    </td>
                </tr>
                <tr>
                    <td>PSIC/PSOC:</td>
                    <td>
                        <span t-field="doc.psic_psoc"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">LL/CAS Permit No:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.ll_cas_permit_no"/>
                    </td>
                </tr>
                <tr>
                    <td>Is the Company using any
                        point of sale (POS)
                        cash register (CRM) or<br/>special purpose (SPM)
                        Machines:
                    </td>
                    <td>
                        <span t-field="doc.ask"/>
                    </td>
                </tr>
            </table>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <h2>Tax Type</h2>
                    <td>
                        <t t-if="doc.income_tax">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.income_tax">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Income Tax
                    </td>
                    <td>
                        <t t-if="doc.withholding_tax_compensation">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.withholding_tax_compensation">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Withholding Tax - Compensation
                    </td>
                    <td>
                        <t t-if="doc.excise_tax">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.excise_tax">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Excise Tax
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.value_added_tax">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.value_added_tax">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Value Added Tax
                    </td>
                    <td>
                        <t t-if="doc.withholding_tax_expanded">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.withholding_tax_expanded">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Withholding Tax Expanded
                    </td>
                    <td>
                        <t t-if="doc.registration_fee">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.registration_fee">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Registration Fee
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.other_percentage_tax">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.other_percentage_tax">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Other Percentage Tax
                    </td>
                    <td>
                        <t t-if="doc.withholding_tax_final">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.withholding_tax_final">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Withholding Tax Final
                    </td>
                </tr>
            </table>
            <br/>
            <h1>
                IV. SEC/DTI Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>Registration No:</td>
                    <td>
                        <span t-field="doc.registration_number"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Trade Name:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.trade_name"/>
                    </td>
                </tr>
                <tr>
                    <td>Registration Date:</td>
                    <td>
                        <span t-field="doc.registration_date"/>
                    </td>
                </tr>
            </table>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <h2>Capitalization</h2>
                    <t t-foreach="docs" t-as="o">
                        <!--                       <span t-field="o.capital_domestic_id.name"/>-->
                    </t>
                </tr>
            </table>
            <br/>
            <h1>
                V. Regulatory Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>Is the entity regulated or
                        accredited by or
                        is license holder issued by any of the following
                        Government Regulatory Agencies:
                    </td>
                    <td>
                        <span t-field="doc.ask_3"/>
                    </td>
                </tr>
            </table>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <h2>Regulatory Agencies</h2>
                    <td>
                        <t t-if="doc.bangko_sentral_pilipinas">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.bangko_sentral_pilipinas">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Bangko Sentral ng Pilipinas
                    </td>
                    <td>
                        <t t-if="doc.land_transportation_franchising_regulatory_board">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.land_transportation_franchising_regulatory_board">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Land Transportation Franchising and Regulatory Board
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.bureau_of_custom">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.bureau_of_custom">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Bureau of Custom
                    </td>
                    <td>
                        <t t-if="doc.philippine_amusement_gaming_corporation">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.philippine_amusement_gaming_corporation">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Philippine Amusement and Gaming Corporation
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.construction_industry_authority_philippines">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.construction_industry_authority_philippines">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Construction Industry authority of the Philippines (PCAB)
                    </td>
                    <td>
                        <t t-if="doc.philippines_council_ngo_certification">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.philippines_council_ngo_certification">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Philippine Council for NGO Certification
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.cooperative_development_authority">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.cooperative_development_authority">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Cooperative Development Authority
                    </td>
                    <td>
                        <t t-if="doc.philippine_amusement_gaming_corporation">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.philippine_amusement_gaming_corporation">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Philippine Amusement and Gaming Corporation
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.insurance_commission">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.insurance_commission">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Insurance Commission
                    </td>
                    <td>
                        <t t-if="doc.professional_regulation_commission">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.professional_regulation_commission">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Professional Regulation Commission
                    </td>
                </tr>
                <tr>
                    <td>
                        <t t-if="doc.integrated_bar_philippines">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.integrated_bar_philippines">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Integrated Bar of the Philippines
                    </td>
                    <td>
                        <t t-if="doc.others_ri">
                            <input type="checkbox" id="checkbox_true" checked="True"/>
                        </t>
                        <t t-if="not doc.others_ri">
                            <input type="checkbox" id="checkbox_false"/>
                        </t>
                        Others
                    </td>
                </tr>
            </table>
            <br/>
            <h1>
                VI. Social Security Information
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <td>SSS ER No:</td>
                    <td>
                        <span t-field="doc.sss"/>
                    </td>
                    <td>Filing:</td>
                    <td>
                        <span t-field="doc.sss_filing"/>
                    </td>
                    <td>Payment:</td>
                    <td>
                        <span t-field="doc.sss_pay"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">PHIC ER No:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.phic"/>
                    </td>
                    <td style="background-color: #dce6f5;">Filing:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.phic_filing"/>
                    </td>
                    <td style="background-color: #dce6f5;">Payment:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.phic_pay"/>
                    </td>
                </tr>
                <tr>
                    <td>HDMF ER No:</td>
                    <td>
                        <span t-field="doc.hdmf"/>
                    </td>
                    <td>Filing:</td>
                    <td>
                        <span t-field="doc.hdmf_filing"/>
                    </td>
                    <td>Payment:</td>
                    <td>
                        <span t-field="doc.hdmf_pay"/>
                    </td>
                </tr>
            </table>
            <br/>
            <h1>
                VII. Engagement Administration
            </h1>
            <br/>
            <table style="font-size:20px;" width="100%">
                <tr>
                    <h2>Engagement Team</h2>
                    <td>Associate:</td>
                    <td>
                        <span t-field="doc.user_id"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Supervisor:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.supervisor_id"/>
                    </td>
                </tr>
                <tr>
                    <td>Manager:</td>
                    <td>
                        <span t-field="doc.manager_id"/>
                    </td>
                </tr>
                <tr>
                    <td style="background-color: #dce6f5;">Lead Partner:</td>
                    <td style="background-color: #dce6f5;">
                        <span t-field="doc.lead_partner_id"/>
                    </td>
                </tr>
                <tr>
                    <td>Cluster:</td>
                    <td>
                        <span t-field="doc.cluster_id"/>
                    </td>
                </tr>
            </table>
        </t>
    </template>

    <template id="report_clientprofile_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="amyu1.report_clientprofile_document"/>
            </t>
        </t>
    </template>

    <record id="paperformat_custom" model="report.paperformat">
        <field name="name">Custom Paper</field>
        <field name="default" eval="True"/>
        <field name="format">custom</field>
        <field name="page_height">297</field>
        <field name="page_width">210</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">10.00</field>
        <field name="margin_bottom">10.00</field>
        <field name="margin_left">7.0</field>
        <field name="margin_right">7.0</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">35</field>
        <field name="dpi">125</field>
    </record>

    <record id="action_report_client_profile" model="ir.actions.report">
        <field name="name">Preview</field>
        <field name="model">client.profile</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">amyu1.report_clientprofile_template</field>
        <field name="report_file">amyu1.cpms_preview_reports</field>
        <field name="print_report_name">'Client Profile - %s' % object.name</field>
        <field name="binding_model_id" ref="model_client_profile"/>
        <field name="binding_type">report</field>
        <!--        <field name="paperformat_id" ref="amyu1.paperformat_custom"/>-->
    </record>
</odoo>
