<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="vcd_supervisor_group" model="res.groups">
        <field name="name">VCD Supervisor</field>
    </record>

    <record id="joc_supervisor_group" model="res.groups">
        <field name="name">JOC Supervisor</field>
    </record>

    <record id="joc_user_group" model="res.groups">
        <field name="name">JOC Users</field>
<!--        <field name="implied_ids" eval="[(4, ref('joc_supervisor_group'))]"/>-->
    </record>

    <record id="vcd_user_group" model="res.groups">
        <field name="name">VCD Users</field>
<!--        <field name="implied_ids" eval="[(4, ref('vcd_supervisor_group'))]"/>-->
    </record>

    <record id="joc_supervisor_access" model="ir.rule">
        <field name="name">Supervisor JOC</field>
        <field name="model_id" ref="model_client_profile"/>
        <field name="domain_force">[('associates_supervisor','=',user.id)]</field>
        <field name="groups" eval="[(4, ref('joc_supervisor_group'))]"/>
    </record>

    <record id="vcd_supervisor_access" model="ir.rule">
        <field name="name">Supervisor VCD</field>
        <field name="model_id" ref="model_client_profile"/>
        <field name="domain_force">[('associates_supervisor','=',user.id)]</field>
        <field name="groups" eval="[(4, ref('vcd_supervisor_group'))]"/>
    </record>

    <record id="joc_supervisor_associate_profile" model="ir.rule">
        <field name="name">Supervisor JOC Associate</field>
        <field name="model_id" ref="model_associates_profile"/>
        <field name="domain_force">[('associates_supervisor','=',user.id)]</field>
        <field name="groups" eval="[(4, ref('joc_supervisor_group'))]"/>
    </record>

    <record id="vcd_supervisor_associate_profile" model="ir.rule">
        <field name="name">Supervisor VCD Associate</field>
        <field name="model_id" ref="model_associates_profile"/>
        <field name="domain_force">[('associates_supervisor','=',user.id)]</field>
        <field name="groups" eval="[(4, ref('vcd_supervisor_group'))]"/>
    </record>

</odoo>